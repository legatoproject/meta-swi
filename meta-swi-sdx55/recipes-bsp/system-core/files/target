TARGET_DIR=/target # default directory
if [ ! -f $TARGET_DIR ]; then
	TARGET_DIR=/sys/devices/soc0/machine
fi

case `cat ${TARGET_DIR}` in
	*9x15* )
		res="9x15"
		;;
	*9x25* )
		res="9x25"
		;;
	*9x35* )
		res="9x35"
		;;
	*9607* )
		res="9607"
		;;
	*8909* | *8009* )
		res="8909"
		;;
        *8917* | *8017* )
                res="8017"
                ;;
	*8916* | *8016* )
                res="8916"
                ;;
	*9640* )
		res="9640";
		;;
	*9650* )
		res="9650";
		;;
        *sdx20* )
		res="sdx20";
                ;;
	*8953* | *8053* )
                res="8053";
                ;;
	*8996* | *8096* )
                res="8096";
                ;;
	*605* )
		res="qcs605";
                ;;
        *405* | *403* )
                res="qcs40x";
		;;
	*8098* )
		res="8098";
		;;
	*sdxpoorwills* )
		res="sdxpoorwills";
		;;
	*sdxprairie* )
		res="sdxprairie";
		;;
	*swi-sdx55* )
		res="sdxprairie";
		;;
	*8150* | *8155* )
		res="sm8150";
		;;
	* )
		res="none"
		;;
esac
echo $res
